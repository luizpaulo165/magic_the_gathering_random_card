{
  "name": "MRC - MTG Random Card",
  "description": "Extension based on MTG cards.",
  "version": "1.1.7",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://scryfall.com/*",
    "https://fonts.googleapis.com/*"
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png",
    "320": "./icons/icon320.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "tab/tab.html"
  }
}